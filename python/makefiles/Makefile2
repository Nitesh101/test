main:dynamic/fact.o dynamic/fib.o dynamic/prime.o dynamicmain.o dynamic/libdyna.so
	gcc -o main dynamic/fact.o dynamic/fib.o dynamic/prime.o dynamicmain.o -I./dynamic -Ldynamic/ -ldyna
dynamicmain.o:dynamicmain.c
	gcc -c dynamicmain.c -Idynamicmain.o
dynamic/fact.o:dynamic/fact.c
	gcc -c -Wall -fPIC dynamic/fact.c -o dynamic/fact.o
dynamic/fib.o:dynamic/fib.c
	gcc -c -Wall -fPIC dynamic/fib.c -o dynamic/fib.o
dynamic/prime.o:dynamic/prime.c
	gcc -c -Wall -fPIC  dynamic/prime.c -o dynamic/prime.o
dynamic/libdyna.so:dynamic/fact.o dynamic/fib.o dynamic/prime.o 
	gcc -shared -o  dynamic/libdyna.a dynamic/fact.o dynamic/fib.o dynamic/prime.o 
clean:
	rm static/*.o
	rm main

